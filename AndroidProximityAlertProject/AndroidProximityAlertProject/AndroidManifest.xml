<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="test.app.syn"
    android:versionCode="1"
    android:versionName="1.0" >
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:icon="@drawable/icon"
        android:label="@string/app_name" >
        <activity
            android:name="test.app.syn.ProxAlertActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.PROX" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="test.app.syn.LbsGeocodingActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.GPS1" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        
        <activity
            android:name="test.app.syn.Mainapp1Activity"
            android:label="@string/title_activity_mainapp1" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="test.app.syn.AndroidHttpPostActivity"
            android:label="@string/title_activity_android_http_post" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="test.app.syn.Mainapp1Activity" />
        </activity>
    </application>

    <uses-sdk android:minSdkVersion="3" />

    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_MOCK_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.VIBRATE" />

</manifest>